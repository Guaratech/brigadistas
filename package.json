{
  "name": "Whatsapp",
  "description": "Whatsapp: An Ionic project",
  "scripts": {
    "build": "cd api & meteor build ../build",
    "deploy": "git pull && gulp build && cp -R www api/public && cd api && passenger restart",
    "install": "typings install",
    "install-global": "ypings install registry:env/meteor --global --save",
    "install-plugins": "cordova plugin add cordova-plugin-console cordova-plugin-device cordova-plugin-splashscreen cordova-plugin-statusbar cordova-plugin-whitelist ionic-plugin-keyboard",
    "format-patch": "git format-patch step-by-step --root --stdout > tutorial/step-by-step.patch && git add tutorial/step-by-step.patch && git commit -m \"Format patch file for step-by-step branch\""
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "^2.0.0-rc.2",
    "accounts-base-client-side": "^0.1.1",
    "accounts-password-client-side": "^0.3.2",
    "angular2-meteor": "^0.6.2",
    "angular2-moment": "^0.8.1",
    "es6-shim": "^0.35.0",
    "ionic-angular": "2.0.0-beta.11",
    "ionic-native": "1.3.2",
    "ionicons": "3.0.0",
    "meteor-client-side": "^1.3.4",
    "meteor-node-stubs": "^0.2.3",
    "moment": "^2.14.1",
    "ng2-translate": "2.2.2",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "del": "2.2.0",
    "es6-promise": "^3.3.1",
    "gulp": "3.9.1",
    "gulp-watch": "4.3.5",
    "ionic-gulp-fonts-copy": "^1.0.0",
    "ionic-gulp-html-copy": "^1.0.0",
    "ionic-gulp-sass-build": "^1.0.0",
    "ionic-gulp-scripts-copy": "^2.0.0",
    "ionic-gulp-tslint": "^1.1.0",
    "ionic-gulp-webpack": "^2.0.0",
    "lodash.camelcase": "^4.1.1",
    "lodash.upperfirst": "^4.2.0",
    "run-sequence": "1.1.5",
    "ts-loader": "^0.8.2",
    "tslint-ionic-rules": "0.0.3",
    "typescript": "^2.0.3",
    "webpack": "^1.13.1"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ]
}
