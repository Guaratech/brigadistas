{
  "name": "BrigadistaCivil",
  "description": "BrigadistaCivil: App that connects civil fire brigades with civil society to fight against the fire.",
  "scripts": {
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "test": "mocha --opts mocha.opts",
    "check": "eval npm install",
    "build": "gulp build",
    "clean": "cordova clean",
    "watch": "gulp watch",
    "dev": "ionic serve && supervisor server/server.js & ",
    "startandroid": "ionic run android",
    "deploy": "git pull && gulp build && 'cp' -R www/* api/public/ && cd api && sed -i -e 's/localhost:3000/app.brigadistacivil.com.br:3000/g' public/build/js/app.bundle.js && passenger-config restart-app",
    "logs": "tail -f api/passenger.3000.log",
    "updatecordova": "cordova platform update android && cordova platform update ios",
    "typings-install": "typings install",
    "typings-definition": "tsd query meteor/practicalmeteor:chai --save --action install",
    "install-global": "typings install registry:env/meteor --global --save",
    "install-plugins": "cordova plugin add cordova-plugin-console cordova-plugin-device cordova-plugin-splashscreen cordova-plugin-statusbar cordova-plugin-whitelist ionic-plugin-keyboard",
    "format-patch": "git format-patch step-by-step --root --stdout > tutorial/step-by-step.patch && git add tutorial/step-by-step.patch && git commit -m \"Format patch file for step-by-step branch\"",
    "createpage": "ionic g page myPage",
    "createprovider": "ionic g provider MyData",
    "deviceshow": "adb devices",
    "servelab": "ionic serve --lab",
    "gen": "npm run gen:release && npm run gen:jarsign && npm run gen:zipalign",
    "gen:release": "cordova build --release android",
    "gen:keytool": "keytool -genkey -v -keystore brigadistas.keystore -alias brigadistas -keyalg RSA -keysize 2048 -validity 10000",
    "gen:jarsign": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore brigadistas.keystore platforms/android/build/outputs/apk/android-armv7-release-unsigned.apk brigadistas",
    "gen:zipalign": "rm -f Brigadistas.apk && /Users/mariohmol/projetos/android-sdk-macosx/build-tools/19.1.0/zipalign -v 4 platforms/android/build/outputs/apk/android-armv7-release-unsigned.apk Brigadistas.apk",
    "gen:lookkey": "keytool -list -v -keystore brigadistas.keystore"
  },
